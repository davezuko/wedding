{
  "name": "wedding",
  "version": "1.0.0",
  "main": "views/index.pug",
  "license": "MIT",
  "scripts": {
    "gulp": "gulp --gulpfile ./build/gulpfile.js --cwd .",
    "firebase-to-csv": "node -r esm -r dotenv/config build/scripts/firebase-to-csv",
    "csv-to-firebase": "node -r esm -r dotenv/config build/scripts/csv-to-firebase",
    "make": "yarn gulp make",
    "start": "yarn gulp start"
  },
  "prettier": {
    "semi": false,
    "bracketSpacing": false,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "postcss": {
    "plugins": {
      "postcss-import": {},
      "postcss-cssnext": {},
      "cssnano": {
        "autoprefixer": false
      }
    }
  },
  "browserslist": [
    "last 2 versions",
    "IE 10",
    "IE 11"
  ],
  "dependencies": {
    "axios": "^0.18.0",
    "bootstrap": "^4.1.1",
    "classnames": "^2.2.6",
    "csv-parse": "^2.5.0",
    "dotenv": "^6.0.0",
    "esm": "^3.0.62",
    "express": "^4.16.3",
    "http-errors": "^1.6.3",
    "morgan": "^1.9.0",
    "pg": "^7.4.3",
    "preact": "^8.2.9",
    "pug": "^2.0.3",
    "winston": "^3.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "cssnano": "^3.10.0",
    "del": "^3.0.0",
    "gaze": "^1.1.3",
    "gulp": "^4.0.0",
    "gulp-if": "^2.0.2",
    "gulp-plumber": "^1.2.0",
    "gulp-postcss": "^7.0.1",
    "postcss": "^6.0.22",
    "postcss-cssnext": "^3.1.0",
    "postcss-import": "^11.1.0",
    "prettier": "^1.13.7",
    "tiny-lr": "^1.1.1",
    "vinyl-named-with-path": "^1.0.0",
    "webpack": "^4.14.0",
    "webpack-stream": "^4.0.3"
  }
}
